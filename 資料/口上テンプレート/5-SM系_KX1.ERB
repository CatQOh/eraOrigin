;-------------------------------------------------
;アイマスク
;-------------------------------------------------
@COM_アイマスク_KX1
;装着時
IF TEQUIP:アイマスク == 0
	;初めて
	IF FCOM:SELECTCOM == 0
		PRINTFORMW 
		FCOM:SELECTCOM = 1
	;二回目以降
	ELSE
		;陥落済み
		IF CFLAG:陥落
			PRINTFORMW 
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
;解除時
ELSEIF TEQUIP:アイマスク
	;初めて（の解除）
	;解除時にしかフラグを処理しないため、装着時のまま調教終了すると解除経験は無いことになる
	IF FCOM:SELECTCOM == 1
		PRINTFORMW 
		FCOM:SELECTCOM = 2
	;2回目以降
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF

RETURN 0

