@EVENT_排卵誘発剤_KX1
SELECTCASE FALLTYPE(TARGET)
	;恋慕、親愛
	CASE 1
		PRINTFORMW 
	;淫乱、娼婦
	CASE 2
		PRINTFORMW 
	;服従、隷属
