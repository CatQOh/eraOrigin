				LOCALS += @"\@ TEQUIP:(TRAINNAME:LCOUNT) ? (終了) # (開始) \@"
			CASE "お風呂場プレイ"
				LOCALS += @"\@ TEQUIP:お風呂場 ? (終了) # (開始) \@"
			CASE "シャワー"
				LOCALS += @"\@ TEQUIP:シャワー ? (停止) # (使用) \@"
