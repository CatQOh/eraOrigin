			CASE "ボールギャグ"
				LOCALS += @"\@ TEQUIP:ボールギャグ ? (解除) # (実行) \@"
			CASE "浣腸器"
				LOCALS += @"\@ TEQUIP:浣腸器 ? (解除) # (使用) \@"
			CASE "拡張バルーン"
				LOCALS += @"\@ TEQUIP:拡張バルーン ? (解除) # (使用) \@"
			CASE "触手口辱"
				LOCALS += @"\@ TEQUIP:触手口辱 ? (解除) # (実行) \@"
