			CASE "触手ペニス責め"
				LOCALS += @"\@ TEQUIP:オナホール ? (解除) # (実行) \@"
			CASE "触手挿入"
				LOCALS += @"\@ TEQUIP:バイブ ? (解除) # (実行) \@"
			CASE "アナル触手"
				LOCALS += @"\@ TEQUIP:アナルバイブ ? (解除) # (実行) \@"
