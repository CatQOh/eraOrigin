		SKIPDISP 1
		TRYCCALLFORM 実行判定_%TRAINNAME:LCOUNT%
			IF RESULT == 0 && TFLAG:実行判定無視 == 0
				SETCOLORBYNAME gray
				LOCALS:(LOCAL%4+1) += "gray"
			ELSE
				SETCOLOR 0xc0c0c0
				LOCALS:(LOCAL%4+1) += "#c0c0c0"
			ENDIF
		CATCH
			SETCOLOR 0xc0c0c0
		ENDCATCH
		SKIPDISP 0

		SELECTCASE TRAINNAME:LCOUNT
			CASE "触手召喚"
				LOCALS '= @"%\@ TEQUIP:触手 ? 触手送還 # 触手召喚 \@, 23%"
			CASEELSE
				LOCALS = %TRAINNAME:LCOUNT%
		ENDSELECT

