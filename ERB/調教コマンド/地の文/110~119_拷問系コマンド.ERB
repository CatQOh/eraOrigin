;115,異物挿入
;-------------------------------------------------
@MESSAGE_COM_異物挿入
PRINTFORM %CALLNAME:TARGET%の
SELECTCASE TFLAG:異物挿入
	CASE GETNUM(ABL, "Ｖ感覚")
		PRINT 膣に
		SELECTCASE TEQUIP:Ｖ異物
			CASE 1
				PRINT 缶を
			CASE 2
				PRINT ペットボトルを
			CASE 3
				PRINT 瓶を
		ENDSELECT
	CASE GETNUM(ABL, "Ａ感覚")
		PRINT アナルに
		SELECTCASE TEQUIP:Ａ異物
			CASE 1
				PRINT 缶を
			CASE 2
				PRINT ペットボトルを
			CASE 3
				PRINT 瓶を
		ENDSELECT
ENDSELECT
PRINTW ねじ込んだ……

SELECTCASE TFLAG:異物挿入
	CASE GETNUM(ABL, "Ｖ感覚")
		SELECTCASE TEQUIP:Ｖ異物
			CASE 1
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%は非常識なほど太い異物の苦痛に悶えている……
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣は水音を立てながら缶を飲み込み、ひんやりとした感触に震えている……
				ENDIF
			CASE 2
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW 薄汚れた不潔なペットボトルを挿入され、%CALLNAME:TARGET%は痛みのあまりに悲鳴を上げている……
				ELSE
					PRINTFORMW 透明なボトルからぽっかりと開いた膣内が露わになり、ボトルの中には少しずつ液が溜まっていった……
				ENDIF
			CASE 3
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%の中に半ば無理やり一升瓶をねじ込むと、あまりの苦痛に%CALLNAME:TARGET%は声にならない悲鳴を上げた……
				ELSE
					PRINTFORMW 一升瓶すらも膣で飲み込んでしまった%CALLNAME:TARGET%は、苦痛と不安が混じったような表情をしている……
				ENDIF
		ENDSELECT
	CASE GETNUM(ABL, "Ａ感覚")
		SELECTCASE TEQUIP:Ａ異物
			CASE 1
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%は非常識なほど太い異物の苦痛に悶えている……
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣は水音を立てながら缶を飲み込み、ひんやりとした感触に震えている……
				ENDIF
			CASE 2
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW 薄汚れた不潔なペットボトルを挿入され、%CALLNAME:TARGET%は痛みのあまりに悲鳴を上げている……
				ELSE
					PRINTFORMW 透明なボトルからぽっかりと開いた腸内が露わになり、ボトルの中には少しずつ腸液が溜まっていった……
				ENDIF
			CASE 3
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%の中に半ば無理やり一升瓶をねじ込むと、あまりの苦痛に%CALLNAME:TARGET%は声にならない悲鳴を上げた……
				ELSE
					PRINTFORMW 一升瓶すらも肛門で飲み込んでしまった%CALLNAME:TARGET%は、苦痛と不安が混じったような表情をしている……
				ENDIF
		ENDSELECT
ENDSELECT

