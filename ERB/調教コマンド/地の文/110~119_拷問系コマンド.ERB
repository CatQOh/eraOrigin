;111,踏む
;-------------------------------------------------
@MESSAGE_COM_踏む
PRINTFORM %CALLNAME:TARGET%の
SELECTCASE TFLAG:踏む部位
	CASE 0
		PRINT 腹
	CASE 1
		PRINT 胸
	CASE 2
		IF TEQUIP:Ｖ異物
			SELECTCASE TEQUIP:Ｖ異物
				CASE 1
					PRINT 缶が挿入された
				CASE 2
					PRINT ペットボトルが挿入された
				CASE 3
					PRINT 一升瓶が挿入された
			ENDSELECT
		ENDIF
		PRINT 股間
	CASE 3
		PRINT 首
	CASE 4
		PRINT 顔
ENDSELECT
