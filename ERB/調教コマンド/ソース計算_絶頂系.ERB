;素質
;-------------------------------------------------
;自制心
IF TALENT:自制心
	TIMES UP:快Ｃ , 0.70
	TIMES UP:快Ｖ , 0.90
	TIMES UP:快Ａ , 1.00
	TIMES UP:快Ｂ , 0.70
ENDIF

;即落ち
IF TALENT:即落ち
	TIMES UP:快Ｃ , 5.00
	TIMES UP:快Ｖ , 5.00
	TIMES UP:快Ａ , 5.00
	TIMES UP:快Ｂ , 5.00
ENDIF
;淫核
SIF TALENT:淫核
	TIMES UP:快Ｃ , 2.00
;淫膣い
SIF TALENT:淫膣
	TIMES UP:快Ｖ , 2.00
;淫尻
SIF TALENT:淫尻
	TIMES UP:快Ａ , 2.00
;淫乳
SIF TALENT:淫乳
	TIMES UP:快Ｂ , 2.00


SELECTCASE SEX(TARGET)
	;男
	CASE 1
		TIMES UP:快Ａ , 1.30
		SIF TALENT:ふたなり
			TIMES UP:快Ｖ , 1.20
	;女
	CASE 2
		;ふたなり
		SIF TALENT:ふたなり
			TIMES UP:快Ｃ , 1.30
ENDSELECT

;性器破損
IF TALENT:性器破損
	UP:苦痛 += UP:快Ｖ/5*3
	TIMES UP:快Ｖ , 0.40
ENDIF

;指挿入れの慣らしギミック用処理、処女もしくは再生処女の時は快Vは99で最大にする
IF (UP:快Ｖ+PALAM:快Ｖ) >= 100 && (TALENT:処女 || TALENT:再生処女)
	UP:快Ｖ = 0
	PALAM:快Ｖ = 99
ENDIF

;ペニス持ちは快Ｃが20%
SIF PENIS(TARGET)
	UP:快Ｃ /= 5

;ペニスがある場合は賢者タイム中じゃないと快Ｃ3000以上にはならない
IF (UP:快Ｃ+PALAM:快Ｃ) >= 3000 && PENIS(TARGET) && (CFLAG:賢者タイム == 0 || NOWEX:射精)
	IF UP:快Ｃ+PALAM:快Ｃ > 3000
		PALAM:快Ｃ = 3000
		UP:快Ｃ = 0
	ENDIF
ENDIF

;-------------------------------------------------
;愛液処理
;-------------------------------------------------
LOCAL = UP:快Ｃ+UP:快Ｖ+UP:快Ａ+UP:快Ｂ
;快Ｃ＋快Ｖ＋快Ａ＋快Ｂが合わせて100以上上昇ならその20%の液体のソースが加わる
IF LOCAL > 100 && VAGINA(TARGET)
	;濡れやすいと濡れにくいはここで処理
	SIF TALENT:濡れやすい
		TIMES LOCAL , 3.00
	SIF TALENT:濡れにくい
		TIMES LOCAL , 0.40
	;種族が精霊で雨が降ってると濡れやすくなる
	SELECTCASE FLAG:天気
		CASE 3 TO 5
			SIF CSTR:種族 == "精霊"
				LOCAL *= 2
	ENDSELECT
	SOURCE:液体 += LOCAL / 5
ENDIF

;-------------------------------------------------
;潮噴き処理
;-------------------------------------------------
SIF CFLAG:賢者タイム > 0 && !NOWEX:射精
	CALL SQUIRT_CHECK

