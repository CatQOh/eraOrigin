	TIMES LOCAL , 1.50
	TIMES LOCAL:1 , 1.20
	UP:潤滑 += LOCAL:1/2
ENDIF

UP:快Ｂ += LOCAL
UP:欲情 += LOCAL:1

;-------------------------------------------------
;同じコマンドの連続実行による減少（快楽系）
;-------------------------------------------------
IF SELECTCOM == PREVCOM
	UP:快Ｃ /= 2
	UP:快Ｖ /= 2
	UP:快Ａ /= 2
	UP:快Ｂ /= 2
ENDIF

;-------------------------------------------------
;気力０による減少（快楽系）
;-------------------------------------------------
IF BASE:気力 <= 0
	TIMES UP:快Ｃ , 0.70
	TIMES UP:快Ｖ , 0.70
	TIMES UP:快Ａ , 0.70
	TIMES UP:快Ｂ , 0.70
ENDIF

;-------------------------------------------------
;相性による補正（快楽系）
;-------------------------------------------------
LOCAL = (RELATION:(NO:PLAYER)-100)/10
IF RELATION:(NO:PLAYER) != 100
	UP:快Ｃ += (UP:快Ｃ/100)*LOCAL
	UP:快Ｖ += (UP:快Ｖ/100)*LOCAL
	UP:快Ａ += (UP:快Ａ/100)*LOCAL
	UP:快Ｂ += (UP:快Ｂ/100)*LOCAL
ENDIF

;-------------------------------------------------
;快感の否定は、快ソースの数値に応じて、抑鬱、反感増加（淫乱、娼婦で無効）
;-------------------------------------------------
IF TALENT:快感の否定 && (!TALENT:淫乱 && !TALENT:娼婦)
	LOCAL = ABL:従順+ABL:欲望+ABL:奉仕精神
	SIF CFLAG:依存度 > 0
		LOCAL += CFLAG:依存度/100
	LOCAL += 10
	SOURCE:鬱屈 += (UP:快Ｃ+UP:快Ｖ+UP:快Ａ+UP:快Ｂ)/LOCAL
	SOURCE:反感 += (UP:快Ｃ+UP:快Ｖ+UP:快Ａ+UP:快Ｂ)/LOCAL
ENDIF

;-------------------------------------------------
;アイテム・素質などによる上下の処理（快楽系）
;-------------------------------------------------
;媚薬
IF CFLAG:媚薬
	UP:快Ｃ += UP:快Ｃ/10*CFLAG:媚薬
	UP:快Ｖ += UP:快Ｖ/10*CFLAG:媚薬
	UP:快Ａ += UP:快Ａ/10*CFLAG:媚薬
	UP:快Ｂ += UP:快Ｂ/10*CFLAG:媚薬
ENDIF

;利尿剤
IF TEQUIP:利尿剤
	TIMES UP:快Ｃ , 1.50
	TIMES UP:快Ｖ , 0.70
	TIMES UP:快Ａ , 0.70
	TIMES UP:快Ｂ , 0.70
ENDIF

;-------------------------------------------------
;陥落素質
;-------------------------------------------------
;淫乱
IF TALENT:淫乱
	TIMES UP:快Ｃ , 1.20
	TIMES UP:快Ｖ , 1.20
	TIMES UP:快Ａ , 1.20
	TIMES UP:快Ｂ , 1.20
ENDIF

;娼婦
IF TALENT:娼婦
	TIMES UP:快Ｃ , 1.50
	TIMES UP:快Ｖ , 1.50
	TIMES UP:快Ａ , 1.50
	TIMES UP:快Ｂ , 1.50
ENDIF

